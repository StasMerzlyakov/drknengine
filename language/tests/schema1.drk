TITLE:
    DESCRIPTION: Алгоритм зажигания лампочек
    START_SKEWER: SK_1

NATIVE_LIBRARY: test

VARIABLES:
    IN I1 INT входная переменная
    OUT I2 INT температура
    OUT B3 BOOL какой-то флаг

ACTIONS:
    A_1:
        DESCRIPTION: Текстовое действие
        CBLOCK: method1 I1
                i method2 B3
                method3 i

    A_2:
        DESCRIPTION: Действие 2
        CBLOCK: method1 I2

    A_3:
        DESCRIPTION: Измерить температуру
        CBLOCK: gett
                gett2

SHELFS:
    S_1:
        DESCRIPTION: cветодиод "питание" := выключен
        EXPRESSION: B3 := False
    S_2:
        DESCRIPTION: установить "температуру" := 50
        EXPRESSION: I2 := 50
    S_3:
        DESCRIPTION: cветодиод "питание" := включен
        EXPRESSION: B3 := True

SKEWERS:
    SK_1:
        ITEMS: S_2 S_3 A_1 A_2 A_3 S_1 END

