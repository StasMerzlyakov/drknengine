TITLE: Тестовый сценарий 1
CLIB: libtest.so.6
DESCRIPTION: 
    TITLE: Алгоритм зажигания лампочек
    VARIABLES:
        IN I1 INT входная переменная 
        OUT I2 INT температура
        OUT B3 BOOL какой-то флаг
ACTIONS:
    A_1:
        TITLE: Текстовое действие
               на две строки
        CBLOCK: method1 I1
                i method2 B3
                method3 i
   A_2:
        TITLE: Действие 2
        CBLOCK: method1 I2    
   A_3:
        TITLE: Измерить температуру
        CBLOCK: I2 gett
SHELFS:
    S_1:
        TITLE: Установить флаг := Включен
        EXPRESSION: B3 := TRUE
SKEWERS:
    SK_1:
        ITEMS: A_1 A_2 A_3 S_1














